# ARCO SearchAPI Configuration
# Rename this file to .env and add your actual keys

# SearchAPI (SerpAPI) Key
SEARCHAPI_KEY=your_serpapi_key_here

# Optional: BigQuery credentials for CrUX integration
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Optional: HTTP Archive API for stack detection
HTTP_ARCHIVE_API_KEY=your_http_archive_key

# Rate limiting configuration
SEARCHAPI_RATE_LIMIT=1.0
SEARCHAPI_MAX_RETRIES=3

# Environment
ENVIRONMENT=development
