# ARCO-FIND Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Application Environment
APP_ENV=development
DEBUG_MODE=False

# Google APIs
# Get from: https://console.developers.google.com/
GOOGLE_PAGESPEED_API_KEY=your_google_pagespeed_key_here
GOOGLE_ADS_API_KEY=your_google_ads_key_here

# Meta Business API
# Get from: https://developers.facebook.com/
META_BUSINESS_API_KEY=your_meta_business_token_here

# Database Configuration
DATABASE_URL=sqlite:///./arco_find.db

# Optional: Additional API Keys for enhanced functionality
# LINKEDIN_API_KEY=your_linkedin_key_here
# TWITTER_API_KEY=your_twitter_key_here

# Security Note: NEVER commit the actual .env file to version control
# The .env file should contain your actual API keys and remain local only